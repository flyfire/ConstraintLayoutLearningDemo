<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black">
    <TextView
        android:id="@+id/motion_transition"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="Transition"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/motion_basic"
        app:layout_constraintVertical_chainStyle="packed"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"/>

    <TextView
        android:id="@+id/motion_basic"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="Basic"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/motion_transition"
        app:layout_constraintBottom_toTopOf="@+id/motion_custom_attribute"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>
    <TextView
        android:id="@+id/motion_custom_attribute"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="CustomAttribute"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/motion_basic"
        app:layout_constraintBottom_toTopOf="@+id/motion_keyframeset_keyposition"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>
    <TextView
        android:id="@+id/motion_keyframeset_keyposition"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="KeyPosition"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/motion_custom_attribute"
        app:layout_constraintBottom_toTopOf="@+id/motion_keyframeset_keyattribute"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>
    <TextView
        android:id="@+id/motion_keyframeset_keyattribute"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="KeyAttribute"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/motion_keyframeset_keyposition"
        app:layout_constraintBottom_toTopOf="@id/motion_keyframeset_keycycle"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>

    <TextView
        android:id="@+id/motion_keyframeset_keycycle"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="KeyCycle"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/motion_keyframeset_keyattribute"
        app:layout_constraintBottom_toTopOf="@id/motion_coordinatelayout"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>
    <TextView
        android:id="@+id/motion_coordinatelayout"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:textSize="16dp"
        android:textColor="@android:color/black"
        android:text="CoordinateLayout"
        android:background="@drawable/round_ffffff_24dp_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/motion_keyframeset_keycycle"
        app:layout_constraintBottom_toBottomOf="parent"
        android:onClick="onClick"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="6dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>